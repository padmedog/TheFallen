        inst_.ip     = as_ip;
        inst_.port   = as_port;
        var inst2_ = instance_create(0,0,obj_server_newplayer);
        inst2_._inst = inst_;
        break;
    case network_type_disconnect:
        var as_sock = ds_map_find_value(async_load,"socket");
        tempArray[0] = as_sock;
        with(obj_playerobject)
        {
            if(socket == other.tempArray[0])
            {
                instance_destroy();
            }
        }
        break;
    default:
        break;
}
